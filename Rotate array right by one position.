#include<stdio.h>
int main()
{
    int arr[]={2,5,9,4,0};
    int size=sizeof(arr)/sizeof(arr[0]);
    int temp=arr[size-1];
    
      for(int i=size-1;i>0;i--)
          {
                arr[i]=arr[i-1];
          }
          arr[0]=temp;
    
     printf("array after rotation:");
     for(int i=0;i<size;i++)
     {
         printf("%d ",arr[i]);
     }
    
    return 0;
}
