#include <stdio.h>

int main(void)
{
    int arr[6];
    int max, smax;

    printf("Enter 6 numbers:\n");
    for (int i = 0; i < 6; i++)
    {
        scanf("%d", &arr[i]);
    }

    max = smax = arr[0];

    for (int i = 1; i < 6; i++)
    {
        if (arr[i] > max)
        {
            smax = max;
            max = arr[i];
        }
        else if (arr[i] > smax && arr[i] != max)
        {
            smax = arr[i];
        }
    }

    printf("Second maximum element is %d\n", smax);

    return 0;
}
