#include <stdio.h>

int main()
{
    int a[] = {1, 2, 3, 4, 5};
    int size = sizeof(a) / sizeof(a[0]);
    int k = 9;

    // Handle cases where k > size
    k = k % size;

    // Right rotate the array k times
    for (int i = 0; i < k; i++)
    {
        int temp = a[size - 1];

        for (int j = size - 1; j > 0; j--)
        {
            a[j] = a[j - 1];
        }

        a[0] = temp;
    }

    // Print the rotated array
    for (int i = 0; i < size; i++)
    {
        printf("%d ", a[i]);
    }

    return 0;
}
