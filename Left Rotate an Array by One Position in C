#include <stdio.h>

int main()
{
    // Initialize array
    int arr[] = {1, 2, 3, 4, 5};
    int size = sizeof(arr) / sizeof(arr[0]);

    // Store first element
    int temp = arr[0];

    // Shift elements to the left
    for (int i = 0; i < size - 1; i++)
    {
        arr[i] = arr[i + 1];
    }

    // Place first element at the end
    arr[size - 1] = temp;

    // Print rotated array
    printf("Array after left rotation by 1:\n");
    for (int i = 0; i < size; i++)
    {
        printf("%d ", arr[i]);
    }

    return 0;
}
