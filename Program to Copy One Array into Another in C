#include <stdio.h>

int main(void)
{
    int a[5] = {34, 87, 90, 67, 56};
    int b[5];

    /* Copy elements from array a to array b */
    for (int i = 0; i < 5; i++)
    {
        b[i] = a[i];
    }

    printf("Array after copying:\n");

    /* Print copied array */
    for (int i = 0; i < 5; i++)
    {
        printf("%d ", b[i]);
    }

    return 0;
}
