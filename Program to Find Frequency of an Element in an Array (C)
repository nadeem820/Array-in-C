#include <stdio.h>

int main(void)
{
    int arr[] = {45, 78, 45, 76, 45, 0};
    int size = sizeof(arr) / sizeof(arr[0]);
    int target = 45;
    int frequency = 0;

    /* Count frequency of target element */
    for (int i = 0; i < size; i++)
    {
        if (arr[i] == target)
        {
            frequency++;
        }
    }

    printf("Target %d appears %d times in the array.\n", target, frequency);

    return 0;
}
