#include <stdio.h>

int main() {
    int a[] = {56, 78, 90, 34, 12};
    int size = sizeof(a) / sizeof(a[0]);
    int temp = 0;

    // Sorting array in ascending order using simple comparison
    for (int i = 0; i < size; i++) {
        for (int j = i + 1; j < size; j++) {
            if (a[i] > a[j]) {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
        }
    }

    // Printing the sorted array
    printf("Sorted Array: ");
    for (int i = 0; i < size; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    return 0;
}
